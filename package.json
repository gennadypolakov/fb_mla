{
  "name": "multilogin-local-rest-api",
  "version": "1.0.0",
  "description": "Multilogin Local REST API can be used in order to start, stop, share, clone browser profile/-s. You can also check if the profile is already running on your machine by using checkProfileRunning endpoint and import cookies by using cookieImportJSON/cookieImportNetscape endpoint.",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "debug": "nodemon --inspect ./bin/www"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "base64-to-image": "^1.0.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "js-cookie": "^2.2.1",
    "js-yaml": "^3.3.0",
    "md5": "^2.2.1",
    "mongoose": "^5.6.9",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "node-schedule": "^1.3.2",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "swagger-tools": "^0.10.4",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  },
  "nodemonConfig": {
    "ignore": [
      "data/*",
      "client/*"
    ],
    "delay": "1000"
  }
}
